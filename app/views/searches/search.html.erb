<h2>Rseults index</h2>

<table class="table table-striped">
  
  <% if @range == "User" %>
    <tbody>
      <% @users.each do |user| %>
       <tr>
     <td><%= user.name %></td>
       </tr>
      <% end %>
  <% else %>
     <% @books.each do |book| %>
       <tr>
         <td><%= book.title %></td>
         <td><%= book.body %></td>
       </tr>
     <% end %>
    </tbody>
  <% end %>
</table>